<?xml version="1.0" encoding="UTF-8"?><plug-in>
    <name>chatbot-botavatar</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>1.0.1-SNAPSHOT</version>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <description>module.chatbot.botavatar.plugin.description</description>
    <provider>botavatar.plugin.provider</provider>
    <provider-url>http://dev.lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/plugins/chatbot/chatbot.png</icon-url>
    <copyright>Copyright (c) 2019 City of Paris</copyright>
    <db-pool-required>1</db-pool-required>
    
    <core-version-dependency>
        <min-core-version>4.1.0</min-core-version>
        <max-core-version/>	
    </core-version-dependency>

    <!-- Admin features parameters -->
    <admin-features>
        <admin-feature>
            <feature-id>BOTAVATAR_MANAGEMENT</feature-id>
            <feature-title>module.chatbot.botavatar.adminFeature.ManageChatbotAvatars.name</feature-title>
            <feature-description>module.chatbot.botavatar.adminFeature.ManageChatbotAvatars.description</feature-description>
            <feature-level>2</feature-level>
            <feature-url>jsp/admin/plugins/chatbot/modules/botavatar/ManageAvatarAssociations.jsp</feature-url>
            <feature-icon-url/>
      </admin-feature>
    </admin-features>    

</plug-in>