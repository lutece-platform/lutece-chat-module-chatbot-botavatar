<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<plug-in>
    <name>botavatar</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>1.0.0-SNAPSHOT</version>
    <documentation></documentation>
    <installation></installation>
    <changes></changes>
    <user-guide></user-guide>
    <description>botavatar.plugin.description</description>
    <provider>botavatar.plugin.provider</provider>
    <provider-url>http://dev.lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/feature_default_icon.png</icon-url>
    <copyright>Copyright (c) 2019 City of Paris</copyright>
    <db-pool-required>1</db-pool-required>
    
    <core-version-dependency>
        <min-core-version>4.1.0</min-core-version>
        <max-core-version/>	
    </core-version-dependency>

    <!-- Admin features parameters -->
    <admin-features>
        <admin-feature>
            <feature-id>BOTAVATAR_MANAGEMENT</feature-id>
            <feature-title>botavatar.adminFeature.ManageChatbotAvatars.name</feature-title>
            <feature-description>botavatar.adminFeature.ManageChatbotAvatars.description</feature-description>
            <feature-level>2</feature-level>
            <feature-url>jsp/admin/plugins/botavatar/ManageAvatarAssociations.jsp</feature-url>
            <feature-icon-url></feature-icon-url>
      </admin-feature>
    </admin-features>    

</plug-in>